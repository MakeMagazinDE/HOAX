; If MACRO is not linked to Tera Term,
testlink
save_result=result
; execute the connect command, use TERATERM.INI params
if save_result=0 connect ''
messagebox "EFX I2C EEPROM maybe disabled by factory (tracks cut due to noise)!" "WARNING"
mypath='C:\HOAX\FPGACORES\hoax3rev.bin'

pause 1
; DataFlash-Bereich loeschen
timeout=2
sendln  'DFS=0!'
waitln '[OK]'
sendln  'DFE 1=10'

; Core 10 = EEPROM-Inhalt fuer FV-1
waitln '000]'
sendln  'DFX 1=10'
waitln '[START XMODEM]'
xmodemsend mypath 1
; EEPROM aus CoreBlock laden und abwarten, I2C-JUMPER SETZEN!
sendln  'SFX=10'
pause 3

sendln  'DFS=1!'

if save_result<>0 end
disconnect
closett

end